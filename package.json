{
  "name": "d3-app",
  "version": "1.0.0",
  "description": "Starter Kit for a d3 application",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com>",
  "homepage": "https://github.com/mauriziocescon",
  "private": false,
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve": "webpack-dev-server --d --progress --profile --colors --env.name=dev | npm run json-server-watch",
    "build": "webpack --progress --profile --colors --env.name=prod",
    "test": "jest src",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/react-app"
  },
  "keywords": [
    "bootstrap",
    "cescon",
    "css-modules",
    "d3",
    "es2017",
    "jest",
    "json-server",
    "maurizio",
    "mock-server",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.4.1",
    "d3": "5.15.0",
    "font-awesome": "4.7.0",
    "jquery": "3.5.0",
    "popper.js": "1.16.1",
    "roboto-fontface": "0.10.0"
  },
  "devDependencies": {
    "@types/bootstrap": "4.3.1",
    "@types/d3": "5.7.2",
    "@types/faker": "4.1.10",
    "@types/jest": "24.9.1",
    "@types/jquery": "3.3.33",
    "@types/node": "12.7.3",
    "@types/webpack": "4.41.6",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-jest": "24.9.0",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.0",
    "css-loader": "1.0.1",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "faker": "4.1.0",
    "file-loader": "5.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "json-server": "0.16.1",
    "node-sass": "4.13.1",
    "nodemon": "2.0.2",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "sass-resources-loader": "2.0.1",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "stylelint": "13.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-webpack-plugin": "1.2.3",
    "sw-precache-webpack-plugin": "1.0.0",
    "ts-jest": "24.3.0",
    "tslint": "6.0.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.8.2",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "4.41.6",
