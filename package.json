{
  "name": "d3-app",
  "version": "1.0.0",
  "description": "Starter Kit for a d3 application",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com>",
  "homepage": "https://github.com/mauriziocescon",
  "private": false,
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve": "webpack serve --progress --color --env name=dev --config webpack.config.js | npm run json-server-watch",
    "build": "webpack --progress --profile --color --env name=prod",
    "test": "jest src",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/d3-app"
  },
  "keywords": [
    "bootstrap",
    "cescon",
    "css-modules",
    "d3",
    "es2017",
    "jest",
    "json-server",
    "maurizio",
    "mock-server",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@fortawesome/fontawesome-free": "6.1.1",
    "@popperjs/core": "2.11.5",
    "bootstrap": "5.1.3",
    "d3": "7.4.4",
    "jquery": "3.6.0",
    "roboto-fontface": "0.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@types/bootstrap": "5.1.12",
    "@types/d3": "7.4.0",
    "@types/faker": "6.6.9",
    "@types/jest": "27.5.1",
    "@types/jquery": "3.5.14",
    "@types/node": "17.0.36",
    "@types/webpack": "5.28.0",
    "babel-jest": "28.1.0",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "expose-loader": "4.0.0",
    "faker": "5.5.3",
    "file-loader": "6.2.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "28.1.0",
    "jest-css-modules": "2.1.0",
    "json-server": "0.17.1",
    "mini-css-extract-plugin": "2.6.0",
    "nodemon": "2.0.20",
    "resolve-url-loader": "5.0.0",
    "sass": "1.52.1",
    "sass-loader": "13.0.0",
    "sass-resources-loader": "2.2.5",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "stylelint": "14.8.5",
    "stylelint-config-standard-scss": "4.0.0",
    "stylelint-webpack-plugin": "3.3.0",
    "ts-jest": "28.0.3",
    "ts-loader": "9.3.0",
    "tslint": "6.1.3",
    "tslint-loader": "3.5.4",
    "typescript": "4.7.2",
    "webpack": "5.72.1",
