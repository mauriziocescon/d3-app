{
  "name": "d3-app",
  "version": "1.0.0",
  "description": "Starter Kit for a d3 application",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com>",
  "homepage": "https://github.com/mauriziocescon",
  "private": false,
  "engines": {
    "node": "=8.11.1",
    "npm": "=5.6.0"
  },
  "scripts": {
    "postinstall": "node ./mock-server/db/db-creator.js | npm run build",
    "start": "npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon ./mock-server/server.js",
    "serve": "webpack-dev-server --d --progress --profile --colors --env.name=dev | npm run json-server",
    "build": "webpack --progress --profile --colors --env.name=prod",
    "test": "jest src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/react-app"
  },
  "keywords": [
    "bootstrap",
    "cescon",
    "css-modules",
    "d3",
    "es2017",
    "jest",
    "json-server",
    "maurizio",
    "mock-server",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.0.0",
    "d3": "5.0.0",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "popper.js": "1.14.3",
    "roboto-fontface": "0.9.0"
  },
  "devDependencies": {
    "@types/bootstrap": "4.0.1",
    "@types/d3": "5.0.0",
    "@types/faker": "4.1.2",
    "@types/jest": "22.2.2",
    "@types/jquery": "3.3.1",
    "@types/node": "9.6.2",
    "@types/popper.js": "1.11.0",
    "awesome-typescript-loader": "5.0.0",
    "babel-core": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.6.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "faker": "4.1.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "22.4.3",
    "json-server": "0.12.1",
    "node-sass": "4.8.3",
    "nodemon": "1.17.3",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "6.0.7",
    "sass-resources-loader": "1.3.3",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.3",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.4",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-jest": "22.4.2",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "typescript": "2.8.1",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3",
    "webpack-manifest-plugin": "2.0.0",
    "webpack-merge": "4.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(less|scss)$": "babel-jest"
    }
  }
}
