{
  "name": "d3-app",
  "version": "1.0.0",
  "description": "Starter Kit for a d3 application",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com>",
  "homepage": "https://github.com/mauriziocescon",
  "private": false,
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve": "webpack serve --progress --color --env name=dev --config webpack.config.js | npm run json-server-watch",
    "build": "webpack --progress --profile --color --env name=prod",
    "test": "jest src",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/d3-app"
  },
  "keywords": [
    "bootstrap",
    "cescon",
    "css-modules",
    "d3",
    "es2017",
    "jest",
    "json-server",
    "maurizio",
    "mock-server",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "bootstrap": "4.6.0",
    "d3": "6.6.2",
    "font-awesome": "4.7.0",
    "jquery": "3.6.0",
    "popper.js": "1.16.1",
    "roboto-fontface": "0.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/preset-env": "7.13.12",
    "@types/bootstrap": "5.0.12",
    "@types/d3": "6.3.0",
    "@types/faker": "5.5.1",
    "@types/jest": "26.0.22",
    "@types/jquery": "3.5.5",
    "@types/node": "14.14.37",
    "@types/webpack": "5.28.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "8.1.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.0",
    "expose-loader": "2.0.0",
    "faker": "5.5.2",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "jest": "26.6.3",
    "jest-css-modules": "2.1.0",
    "json-server": "0.16.3",
    "mini-css-extract-plugin": "1.4.1",
    "node-sass": "5.0.0",
    "nodemon": "2.0.7",
    "resolve-url-loader": "3.1.2",
    "sass-loader": "11.0.1",
    "sass-resources-loader": "2.1.1",
    "source-map-loader": "2.0.1",
    "style-loader": "2.0.0",
    "stylelint": "13.12.0",
    "stylelint-config-standard": "21.0.0",
    "stylelint-webpack-plugin": "2.1.1",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "tslint": "6.1.3",
    "tslint-loader": "3.5.4",
    "typescript": "4.2.4",
    "webpack": "5.31.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.6.0",
